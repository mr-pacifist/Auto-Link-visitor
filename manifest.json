{
  "manifest_version": 3,
  "name": "Automatic Link Visitor",
  "version": "1.0",
  "description": "Visits links at random intervals in the same tab.",
  "permissions": [
    "tabs",
    "alarms",
    "storage"
  ],
  "background": {
    "service_worker": "background.js"
  },
  "action": {
    "default_popup": "popup.html"
  }
}